#include <iostream>
#include <cmath>
using namespace std;

void CUADRATICA(double a, double b, double c) {
    double d = b*b - 4*a*c;
    if (d > 0)
        cout << "Dos raices reales: " << (-b + sqrt(d)) / (2*a) << " y " << (-b - sqrt(d)) / (2*a) << endl;
    else if (d == 0)
        cout << "Una raiz doble: " << -b / (2*a) << endl;
    else
        cout << "Raices complejas: " << -b/(2*a) << " Â± " << sqrt(-d)/(2*a) << "i" << endl;
}

int main() {
    double a,b,c;
    cout << "Ingrese a, b y c: ";
    cin >> a >> b >> c;
    CUADRATICA(a,b,c);
    return 0;
}
